    "ejs": "^3.1.9",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.0",
    "koa-helmet": "^7.0.2",
    "koa-json": "^2.0.2",
    "koa-pino-logger": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^8.0.0",
    "mysql2": "^3.4.0",
